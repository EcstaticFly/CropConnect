<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>header2</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">
    <!-- <meta name="google-signin-client_id" content="982671331990-055bkvp3rui4u51t826cfimjrhc5937m.apps.googleusercontent.com"> -->
    <style>
      header{
        margin-bottom: 0%;
        padding-bottom: 0%;
        border-bottom: 0cap;
        z-index: 1;
      }
      button {
  background: none!important;
  border: none;
  padding-left: 16px;
  padding-right: 16px;
  padding-top: 4px;
  padding-top: 4px;
  /*optional*/
  font-family: arial, sans-serif;
  /*input has OS specific font-family*/
  color: #069;
  text-decoration: underline;
  cursor: pointer;
}
      

    </style>
    
    <!-- <script src="index.js"></script> -->
</head>
<body>
    <header class="p-3 mb-0 border-bottom">
        <div class="container">
          <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
            <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 link-body-emphasis text-decoration-none">
              <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap"></use></svg>
            </a>
    
            <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
              <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                <li><a href="http://localhost:3000/" class="nav-link px-2 text-secondary">Home</a></li>
                <li><a href="#" class="nav-link px-2 text-white">FAQs</a></li>
                <li><a href="#" class="nav-link px-2 text-white">About</a></li>
                <li><form action="/show-cart" method="get"><button type="submit"><i class="fa-solid fa-cart-shopping"></i><%=p%></button></form></li>
              </ul>
            </ul>
    
            <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3" role="search">
              <input type="search" class="form-control" placeholder="Search..." aria-label="Search">
            </form>
    
            <div class="dropdown text-end">
              <a href="#" class="d-block link-body-emphasis text-decoration-none dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
                <img src="https://github.com/mdo.png" alt="mdo" width="32" height="32" class="rounded-circle">
              </a>
              <form action="/view-details" method="POST">
                <ul class="dropdown-menu text-small">
                  <li><button  id="home" class="dropdown-item" name="butt" value="1" >Home</button></li>
                  <li><button  id="orders" class="dropdown-item" name="butt" value="2">Orders</button></li>
                  <li><button  id="profile" class="dropdown-item" name="butt" value="3">Profile</button></li>
                  <li><hr class="dropdown-divider"></li>
                  <li><button type="submit" id="signOut" class="dropdown-item" name="butt" value="4">Sign Out</button></li>
                </ul>
              </form>
            </div>
          </div>
        </div>
      </header>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
       <!-- <script src="index.js"></script> -->
      
      <script src="https://apis.google.com/js/platform.js" async defer></script>
